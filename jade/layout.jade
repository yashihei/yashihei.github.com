doctype html
html(lang="ja")
    head
        meta(charset='utf-8')
        meta(http-equiv="X-UA-Compatible" content="IE=edge")
        meta(name="viewport" content="width=device-width, initial-scale=1")
        block title
            title yashihei.net
        meta(name="description" content="yashihei.netではやしへいが制作したゲームなどの公開をしてます。")
        link(href="css/bootstrap.min.css" rel="stylesheet")
        link(href="css/font-awesome.min.css" rel="stylesheet")
        link(href="css/default.css" rel="stylesheet")
        <!--[if lt IE 9]>
        script(src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js")
        script(src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js")
        <![endif]-->
    body
        nav.navbar.navbar-inverse
            .container
                .navbar-header
                    button.navbar-toggle(data-toggle="collapse" data-target=".target")
                        - for (var i = 0; i < 3; i++)
                            span.icon-bar
                    a.navbar-brand(href="/") yashihei.net
                .collapse.navbar-collapse.target
                    ul.nav.navbar-nav
                        li
                            a(href="./about.html") About
                        li
                            a(target="_brank" href="http://yashihei.hatenablog.com") Blog
        block content
        footer
            div.container
                p.text-center © 2015 yashihei
        script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js")
        script(src="js/bootstrap.min.js")
